{
  "compilerOptions": {
    "lib": ["es2017", "es7", "es6", "dom"],
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
  "include": ["lib/*"],
  "exclude": ["node_modules", "dist"]
}
